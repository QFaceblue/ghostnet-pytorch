{
    "dataset": "mnist",
    "model": "ghostnet",
    "root_train": "D:\\dataset\\mnist",
    "root_eval": "D:\\dataset\\mnist",

    "load": true,
    "save": false,
    "device": [0],
    "num_workers": 0,

    "nbatch_train": 100,
    "nbatch_eval": 100,
    "epoches": 6,

    "optimizer": "adam",
    "lr_base": 0.001,
    "lr_gamma": 0.1,
    "lr_schedule": [2000, 3000],
    "momentum": 0.5,
    "weight_decay": 0.0001,
    "grad_clip": 20,
    "warmup_iter": 0,
    "warmup_factor": 0.33,

    "img_scale_min": 0.7,

    "augmentation": true,
    "random_xflip": {"probability": 0},
    "random_color": {
        "probability": 0,
        "brightness": 0.15,
        "contrast": 0.15,
        "saturation": 0.15,
        "hue": 0.15
    },
    "random_rotation": {"probability": 0.5, "degrees": 20},
    "random_crop": {"probability": 0.5, "size": 26, "padding": 5}
}